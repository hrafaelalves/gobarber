sword,\n    });\n\n    const { token, user } = response.data;\n\n    localStorage.setItem('@GoBarber:token', token);\n    localStorage.setItem('@GoBarber:user', JSON.stringify(user));\n\n    setData({ token, user });\n  }, []);\n\n  const signOut = useCallback(() => {\n    localStorage.removeItem('@GoBarber:token');\n    localStorage.removeItem('@GoBarber:user');\n\n    setData({} as AuthState);\n  }, []);\n\n  return (\n    <AuthContext.Provider value={{ user: data.user, signIn, signOut }}>\n      {children}\n    </AuthContext.Provider>\n  );\n};\n\nfunction useAuth(): AuthContextData {\n  const context = useContext(AuthContext);\n\n  if (!context) {\n    throw new Error('useAuth must be used within an AuthProvider');\n  }\n\n  return context;\n}\n\nexport { AuthProvider, useAuth };\n","/home/hugorafael/develop/produto/curso_rocketseat/gostack/gobarber/web/src/hooks/index.tsx",[],"/home/hugorafael/develop/produto/curso_rocketseat/gostack/gobarber/web/src/hooks/toast.tsx",[],"/home/hugorafael/develop/produto/curso_rocketseat/gostack/gobarber/web/src/components/ToastContainer/Toast/index.tsx",[],"/home/hugorafael/develop/produto/curso_rocketseat/gostack/gobarber/web/src/components/ToastContainer/Toast/styles.ts",[],{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","severity":1,"message":"113","line":18,"column":49,"nodeType":"114","messageId":"115","endLine":18,"endColumn":52,"suggestions":"116"},{"ruleId":"117","severity":1,"message":"118","line":11,"column":21,"nodeType":"119","messageId":"120","endLine":11,"endColumn":32},{"ruleId":"112","severity":1,"message":"113","line":6,"column":9,"nodeType":"114","messageId":"115","endLine":6,"endColumn":12,"suggestions":"121"},{"ruleId":"112","severity":1,"message":"113","line":15,"column":9,"nodeType":"114","messageId":"115","endLine":15,"endColumn":12,"suggestions":"122"},"lines